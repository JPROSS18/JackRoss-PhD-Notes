   "source": [
    "using BifurcationKit, Plots\n",
    "F(x, p) = @. p[1] + x - x^3/3\n",
    "prob = BifurcationProblem(F, [-2.], [-1.], 1;\n",
    "    record_from_solution = (x,p; k...) -> x[1])\n",
    "br = continuation(prob, PALC(), ContinuationPar(p_min = -1., p_max = 1.))\n",
    "plot(br)"
   ]
